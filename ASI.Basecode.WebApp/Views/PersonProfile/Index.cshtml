@model ASI.Basecode.Services.ServiceModels.PersonProfileViewModel;
@{
    ViewData["Title"] = "My Profile";
}

<h2>@Model.FirstName @Model.LastName’s Profile</h2>

<table class="table">
    <tr>
        <th>Username</th>
        <td>@Model.UserId</td>
    </tr>
    <tr>
        <th>First Name</th>
        <td>@Model.FirstName</td>
    </tr>
    <tr>
        <th>Last Name</th>
        <td>@Model.LastName</td>
    </tr>
    <tr>
        <th>Bio</th>
        <td>@Model.AboutMe</td>
    </tr>

</table>
